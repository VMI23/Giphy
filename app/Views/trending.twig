{% extends 'base.twig' %}

{% block body %}
<div class="flex justify-center">
    <h1 class="text-2xl font-bold py-4">Displaying trending gifs:</h1>
</div>
    <div class="flex justify-center items-center flex-wrap h-full">
        {% for gifUrl in gifs %}
            <img class="mx-4" src="{{ gifUrl.getUrl() }}" alt="GIF">
        {% endfor %}
    </div>

{% endblock %}